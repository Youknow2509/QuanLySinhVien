@model IEnumerable<ChuongTrinhHoc>
<!-- Begin Page Content -->
<div class="container-fluid">
    <!-- DataTales Example -->
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">Bảng dữ liệu Chương Trình Học</h6>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th>Tên Chương Trình Học</th>
                        </tr>
                    </thead>
                    <tfoot>
                        <tr>
                            <th>Tên Chương Trình Học</th>
                        </tr>
                    </tfoot>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    <a
                                        asp-area="Admin"
                                        asp-controller="QuanLyChuongTrinhHoc"
                                        asp-action="Details"
                                        asp-route-IdChuongTrinhHoc="@item.IdChuongTrinhHoc"
                                    >
                                        @item.TenChuongTrinhHoc
                                    </a>
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>

</div>
<!-- /.container-fluid -->
