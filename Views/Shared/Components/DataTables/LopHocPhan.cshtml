@model IEnumerable<LopHocPhan>
<!-- Begin Page Content -->
<div class="container-fluid">
    <!-- DataTales Example -->
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">Bảng dữ liệu Lớp Học Phần</h6>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th>Lớp Học Phần</th>
                            <th>Giảng Viên</th>
                            <th>Môn học</th>
                        </tr>
                    </thead>
                    <tfoot>
                        <tr>
                            <th>Lớp Học Phần</th>
                            <th>Giảng Viên</th>
                            <th>Môn học</th>
                        </tr>
                    </tfoot>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.TenHocPhan</td>
                                <td>@item.GiaoViens.TenGiaoVien</td>
                                <td>@item.MonHocs.TenMonHoc</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>

</div>
<!-- /.container-fluid -->
