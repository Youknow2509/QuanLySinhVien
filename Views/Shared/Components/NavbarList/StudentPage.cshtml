@model IEnumerable<ViewNavItem>

@foreach (var navbar in Model)
{
    <li class="nav-item">
        <a class="nav-link" 
            asp-area="@navbar.Area"
            asp-controller="@navbar.Controller"
            asp-action="@navbar.Action"
        >
            <i class="@navbar.IconClass"></i>
            <span>@navbar.Content</span></a>
    </li>
}

